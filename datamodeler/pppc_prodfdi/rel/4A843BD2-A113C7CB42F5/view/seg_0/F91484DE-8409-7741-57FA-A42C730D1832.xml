<userDefinedSQL><![CDATA[CREATE OR REPLACE VIEW GROUP_PP_VIEW
AS 
SELECT
	'I' AS GROUP_PP_COMPANY,
	PROPOSAL_NO AS GROUP_PP_PROPOSAL_NO
FROM
	PROPOSAL
UNION ALL
SELECT
        'N' AS GROUP_PP_COMPANY,
        PROPOSAL_NO AS GROUP_PP_PROPOSAL_NO
FROM
        PROPOSAL@DBA_FCS_FDN.FOCUS.CO.ID]]></userDefinedSQL>
