<userDefinedSQL><![CDATA[CREATE OR REPLACE VIEW FCS_YEARLY_BUDGET_DETAIL_V
AS 
SELECT TBL."NO_URUT",TBL."YB_ID",TBL."YB_TYPE",TBL."YB_YEAR",TBL."YB_CODE",TBL."YB_DESC",TBL."PCATEGORY_CODE",TBL."PCATEGORY_DESC",TBL."PCLASS_CODE",TBL."PCLASS_DESC",TBL."PBRAND_CODE",TBL."PBRAND_DESC",TBL."PEXT_CODE",TBL."PEXT_DESC",TBL."PPACKAGING_CODE",TBL."PPACKAGING_DESC",TBL."PVARIANT_CODE",TBL."PVARIANT_DESC",TBL."STATUS",TBL."TRX_TYPE",TBL."TRX_DATE",TBL."TRX_REMARKS",TBL."TRX_CREATE_BY",TBL."TRX_CREATE_DATE",TBL."TRX_MODIFIED_BY",TBL."TRX_MODIFIED_DATE",TBL."TRX_ACTION",TBL."TRX_ACTION_BY",TBL."TRX_ACTION_DATE",TBL."TRX_AMOUNT",TBL."TRX_%",TBL."YB_AMT",TBL."YB_RMN",TBL."B_TO_DATE_AMT",TBL."B_TO_DATE_USE",TBL."B_OVER_AMT",TBL."B_TO_DATE_RMN" 
FROM
(SELECT 
'001' AS No_URUT,
HDR.BUDGET_CUST_HDR_ID YB_ID,
HDR.BUDGET_TYPE YB_TYPE,
HDR.BUDGET_YEAR YB_YEAR,
CASE WHEN HDR.BUDGET_TYPE = 'CUSTOMER' THEN HDR.CUSTOMER_ID
     WHEN HDR.BUDGET_TYPE = 'POSTING' THEN HDR.KODE_POSTING
ELSE NULL
END AS YB_CODE,
CASE WHEN HDR.BUDGET_TYPE = 'CUSTOMER' THEN
          (SELECT ffv.description
             FROM apps.fnd_flex_value_sets ffvs, apps.fnd_flex_values_vl ffv
            WHERE ffv.flex_value_set_id = ffvs.flex_value_set_id
              AND ffvs.flex_value_set_name = 'FCSAR_CUSTOMER_GROUP'
              AND ffv.flex_value = HDR.CUSTOMER_ID) 
     WHEN HDR.BUDGET_TYPE = 'POSTING' THEN
          (SELECT msi.description
             FROM apps.fcs_inv_view_mstr_item msi
            WHERE msi.item_type = 'PST'
              AND msi.item = HDR.KODE_POSTING)
ELSE NULL
END AS YB_DESC,
DET.BUDGET_CATEGORY PCATEGORY_CODE,
(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_CATEGORY AND MCV.STRUCTURE_NAME = 'FCS_CATEGORY') AS PCATEGORY_DESC,
DET.BUDGET_CLASS PCLASS_CODE,
(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_CLASS AND MCV.STRUCTURE_NAME = 'FCS_CLASS') AS PCLASS_DESC,
DET.BUDGET_BRAND PBRAND_CODE,
(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_BRAND AND MCV.STRUCTURE_NAME = 'FCS_BRAND') AS PBRAND_DESC,
DET.BUDGET_EXTENTION PEXT_CODE,
(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_EXTENTION AND MCV.STRUCTURE_NAME = 'FCS_EXTENTION') AS PEXT_DESC,
DET.BUDGET_PACKAGING PPACKAGING_CODE,
(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_PACKAGING AND MCV.STRUCTURE_NAME = 'FCS_PACKAGING') AS PPACKAGING_DESC,
DET.BUDGET_VARIANT PVARIANT_CODE,
(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_VARIANT AND MCV.STRUCTURE_NAME = 'FCS_VARIANT') AS PVARIANT_DESC,
DET.STATUS,
NULL AS TRX_TYPE,
NULL AS TRX_DATE,
NULL AS TRX_REMARKS,
NULL AS TRX_CREATE_BY,
NULL AS TRX_CREATE_DATE,
NULL AS TRX_MODIFIED_BY,
NULL AS TRX_MODIFIED_DATE,
NULL AS TRX_ACTION,
NULL AS TRX_ACTION_BY,
NULL AS TRX_ACTION_DATE,
NULL AS TRX_AMOUNT,
NULL AS "TRX_%",
DET.YEARLY_BUDGET_AMOUNT YB_AMT,
DET.YEARLY_BUDGET_AMOUNT - DET.BUDGET_AS_TO_DATE YB_RMN,
DET.BUDGET_AS_TO_DATE B_To_Date_AMT,
DET.BUDGET_AS_TO_DATE_USED B_To_Date_USE,
DET.OVER_BUDGET B_Over_AMT,
DET.BUDGET_AS_TO_DATE - DET.BUDGET_AS_TO_DATE_USED - (CASE WHEN DET.OVER_BUDGET IS NULL THEN 0 ELSE DET.OVER_BUDGET END) B_To_Date_Rmn
FROM
FOCUSPP.BUDGET_CUSTOMER DET,
FOCUSPP.BUDGET_CUST_HDR HDR
WHERE
DET.BUDGET_CUST_HDR_ID = HDR.BUDGET_CUST_HDR_ID
UNION ALL
SELECT
'002' AS No_URUT,
DET_TRX_YB.YB_ID,
DET_TRX_YB.YB_TYPE,
DET_TRX_YB.YB_YEAR,
DET_TRX_YB.YB_CODE,
DET_TRX_YB.YB_DESC,
DET_TRX_YB.PCATEGORY_CODE,
DET_TRX_YB.PCATEGORY_DESC,
DET_TRX_YB.PCLASS_CODE,
DET_TRX_YB.PCLASS_DESC,
DET_TRX_YB.PBRAND_CODE,
DET_TRX_YB.PBRAND_DESC,
DET_TRX_YB.PEXT_CODE,
DET_TRX_YB.PEXT_DESC,
DET_TRX_YB.PPACKAGING_CODE,
DET_TRX_YB.PPACKAGING_DESC,
DET_TRX_YB.PVARIANT_CODE,
DET_TRX_YB.PVARIANT_DESC,
DET_TRX_YB.STATUS,
NULL AS TRX_TYPE,
NULL AS TRX_DATE,
NULL AS TRX_REMARKS,
NULL AS TRX_CREATE_BY,
NULL AS TRX_CREATE_DATE,
NULL AS TRX_MODIFIED_BY,
NULL AS TRX_MODIFIED_DATE,
NULL AS TRX_ACTION,
NULL AS TRX_ACTION_BY,
NULL AS TRX_ACTION_DATE,
CASE WHEN DET_TRX_YB.YB_TYPE = 'CUSTOMER' THEN SUM(DET_TRX_YB.TRX_AMOUNT)
ELSE NULL
END AS TRX_AMOUNT,
CASE WHEN DET_TRX_YB.YB_TYPE = 'CUSTOMER' THEN SUM(DET_TRX_YB."TRX_%")
ELSE NULL
END AS "TRX_%",
CASE WHEN DET_TRX_YB.YB_TYPE = 'CUSTOMER' THEN SUM(DET_TRX_YB.TRX_AMOUNT) * SUM(DET_TRX_YB."TRX_%") / 100
     WHEN DET_TRX_YB.YB_TYPE = 'POSTING' THEN SUM(DET_TRX_YB.YB_AMT)
ELSE NULL
END AS YB_AMT,
CASE WHEN DET_TRX_YB.YB_TYPE = 'CUSTOMER' THEN (SUM(DET_TRX_YB.TRX_AMOUNT) * SUM(DET_TRX_YB."TRX_%") / 100) - SUM(DET_TRX_YB.B_To_Date_AMT)
     WHEN DET_TRX_YB.YB_TYPE = 'POSTING' THEN SUM(DET_TRX_YB.YB_AMT) - SUM(DET_TRX_YB.B_To_Date_AMT)
ELSE NULL
END AS YB_RMN,
SUM(DET_TRX_YB.B_To_Date_AMT) B_To_Date_AMT,
NULL AS B_To_Date_USE,
NULL AS B_Over_AMT,
NULL AS B_To_Date_RMN
FROM
(SELECT
HDR.BUDGET_CUST_HDR_ID YB_ID,
HDR.BUDGET_TYPE YB_TYPE,
HDR.BUDGET_YEAR YB_YEAR,
CASE WHEN HDR.BUDGET_TYPE = 'CUSTOMER' THEN HDR.CUSTOMER_ID
     WHEN HDR.BUDGET_TYPE = 'POSTING' THEN HDR.KODE_POSTING
ELSE NULL
END AS YB_CODE,
CASE WHEN HDR.BUDGET_TYPE = 'CUSTOMER' THEN
          (SELECT ffv.description
             FROM apps.fnd_flex_value_sets ffvs, apps.fnd_flex_values_vl ffv
            WHERE ffv.flex_value_set_id = ffvs.flex_value_set_id
              AND ffvs.flex_value_set_name = 'FCSAR_CUSTOMER_GROUP'
              AND ffv.flex_value = HDR.CUSTOMER_ID) 
     WHEN HDR.BUDGET_TYPE = 'POSTING' THEN
          (SELECT msi.description
             FROM apps.fcs_inv_view_mstr_item msi
            WHERE msi.item_type = 'PST'
              AND msi.item = HDR.KODE_POSTING)
ELSE NULL
END AS YB_DESC,
DET.BUDGET_CATEGORY PCATEGORY_CODE,
(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_CATEGORY AND MCV.STRUCTURE_NAME = 'FCS_CATEGORY') AS PCATEGORY_DESC,
DET.BUDGET_CLASS PCLASS_CODE,
(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_CLASS AND MCV.STRUCTURE_NAME = 'FCS_CLASS') AS PCLASS_DESC,
DET.BUDGET_BRAND PBRAND_CODE,
(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_BRAND AND MCV.STRUCTURE_NAME = 'FCS_BRAND') AS PBRAND_DESC,
DET.BUDGET_EXTENTION PEXT_CODE,
(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_EXTENTION AND MCV.STRUCTURE_NAME = 'FCS_EXTENTION') AS PEXT_DESC,
DET.BUDGET_PACKAGING PPACKAGING_CODE,
(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_PACKAGING AND MCV.STRUCTURE_NAME = 'FCS_PACKAGING') AS PPACKAGING_DESC,
DET.BUDGET_VARIANT PVARIANT_CODE,
(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_VARIANT AND MCV.STRUCTURE_NAME = 'FCS_VARIANT') AS PVARIANT_DESC,
DET.STATUS,
DET_TRX.BUDGET_TYPE TRX_TYPE,
DET_TRX.BUDGET_DATE TRX_DATE,
DET_TRX.REMARKS TRX_REMARKS,
DET_TRX.CREATED_BY TRX_CREATE_BY,
DET_TRX.CREATION_DATE TRX_CREATE_DATE,
DET_TRX.MODIFIED_BY TRX_MODIFIED_BY,
DET_TRX.MODIFIED_DATE TRX_MODIFIED_DATE,
DET_TRX.ACTION TRX_ACTION,
DET_TRX.ACTION_BY TRX_ACTION_BY,
DET_TRX.ACTION_DATE TRX_ACTION_DATE,
CASE WHEN HDR.BUDGET_TYPE = 'CUSTOMER' AND DET_TRX.BUDGET_TYPE = 'BALANCE' THEN DET_TRX.AMOUNT
ELSE NULL
END AS TRX_AMOUNT,
CASE WHEN HDR.BUDGET_TYPE = 'CUSTOMER' AND DET_TRX.BUDGET_TYPE = 'BALANCE' THEN DET_TRX.PERCENTAGE
ELSE NULL
END AS "TRX_%",
CASE WHEN HDR.BUDGET_TYPE = 'POSTING' AND DET_TRX.BUDGET_TYPE = 'BALANCE' THEN DET_TRX.AMOUNT
ELSE NULL
END AS YB_AMT,
NULL AS YB_RMN,
CASE WHEN DET_TRX.BUDGET_TYPE = 'BALANCE' THEN 0
     WHEN DET_TRX.BUDGET_TYPE = 'ADJUSTMENT' THEN DET_TRX.AMOUNT
ELSE NULL
END AS B_To_Date_AMT,
NULL AS B_To_Date_USE,
NULL AS B_Over_AMT,
NULL AS B_To_Date_RMN
FROM
FOCUSPP.BUDGET_CUST_TRAN DET_TRX,
FOCUSPP.BUDGET_CUSTOMER DET,
FOCUSPP.BUDGET_CUST_HDR HDR
WHERE
DET.BUDGET_CUST_HDR_ID = HDR.BUDGET_CUST_HDR_ID
AND DET.BUDGET_CUSTOMER_ID = DET_TRX.BUDGET_CUSTOMER_ID) DET_TRX_YB
WHERE 
DET_TRX_YB.TRX_ACTION = 'APPROVED'
GROUP BY
DET_TRX_YB.YB_ID,
DET_TRX_YB.YB_TYPE,
DET_TRX_YB.YB_YEAR,
DET_TRX_YB.YB_CODE,
DET_TRX_YB.YB_DESC,
DET_TRX_YB.PCATEGORY_CODE,
DET_TRX_YB.PCATEGORY_DESC,
DET_TRX_YB.PCLASS_CODE,
DET_TRX_YB.PCLASS_DESC,
DET_TRX_YB.PBRAND_CODE,
DET_TRX_YB.PBRAND_DESC,
DET_TRX_YB.PEXT_CODE,
DET_TRX_YB.PEXT_DESC,
DET_TRX_YB.PPACKAGING_CODE,
DET_TRX_YB.PPACKAGING_DESC,
DET_TRX_YB.PVARIANT_CODE,
DET_TRX_YB.PVARIANT_DESC,
DET_TRX_YB.STATUS
UNION ALL
SELECT
'003' AS No_URUT,
HDR.BUDGET_CUST_HDR_ID YB_ID,
HDR.BUDGET_TYPE YB_TYPE,
HDR.BUDGET_YEAR YB_YEAR,
CASE WHEN HDR.BUDGET_TYPE = 'CUSTOMER' THEN HDR.CUSTOMER_ID
     WHEN HDR.BUDGET_TYPE = 'POSTING' THEN HDR.KODE_POSTING
ELSE NULL
END AS YB_CODE,
CASE WHEN HDR.BUDGET_TYPE = 'CUSTOMER' THEN
          (SELECT ffv.description
             FROM apps.fnd_flex_value_sets ffvs, apps.fnd_flex_values_vl ffv
            WHERE ffv.flex_value_set_id = ffvs.flex_value_set_id
              AND ffvs.flex_value_set_name = 'FCSAR_CUSTOMER_GROUP'
              AND ffv.flex_value = HDR.CUSTOMER_ID) 
     WHEN HDR.BUDGET_TYPE = 'POSTING' THEN
          (SELECT msi.description
             FROM apps.fcs_inv_view_mstr_item msi
            WHERE msi.item_type = 'PST'
              AND msi.item = HDR.KODE_POSTING)
ELSE NULL
END AS YB_DESC,
DET.BUDGET_CATEGORY PCATEGORY_CODE,
(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_CATEGORY AND MCV.STRUCTURE_NAME = 'FCS_CATEGORY') AS PCATEGORY_DESC,
DET.BUDGET_CLASS PCLASS_CODE,
(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_CLASS AND MCV.STRUCTURE_NAME = 'FCS_CLASS') AS PCLASS_DESC,
DET.BUDGET_BRAND PBRAND_CODE,
(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_BRAND AND MCV.STRUCTURE_NAME = 'FCS_BRAND') AS PBRAND_DESC,
DET.BUDGET_EXTENTION PEXT_CODE,
(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_EXTENTION AND MCV.STRUCTURE_NAME = 'FCS_EXTENTION') AS PEXT_DESC,
DET.BUDGET_PACKAGING PPACKAGING_CODE,
(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_PACKAGING AND MCV.STRUCTURE_NAME = 'FCS_PACKAGING') AS PPACKAGING_DESC,
DET.BUDGET_VARIANT PVARIANT_CODE,
(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_VARIANT AND MCV.STRUCTURE_NAME = 'FCS_VARIANT') AS PVARIANT_DESC,
DET.STATUS,
DET_TRX.BUDGET_TYPE TRX_TYPE,
DET_TRX.BUDGET_DATE TRX_DATE,
DET_TRX.REMARKS TRX_REMARKS,
DET_TRX.CREATED_BY TRX_CREATE_BY,
DET_TRX.CREATION_DATE TRX_CREATE_DATE,
DET_TRX.MODIFIED_BY TRX_MODIFIED_BY,
DET_TRX.MODIFIED_DATE TRX_MODIFIED_DATE,
DET_TRX.ACTION TRX_ACTION,
DET_TRX.ACTION_BY TRX_ACTION_BY,
DET_TRX.ACTION_DATE TRX_ACTION_DATE,
CASE WHEN HDR.BUDGET_TYPE = 'CUSTOMER' AND DET_TRX.BUDGET_TYPE = 'BALANCE' THEN DET_TRX.AMOUNT
ELSE NULL
END AS TRX_AMOUNT,
CASE WHEN HDR.BUDGET_TYPE = 'CUSTOMER' AND DET_TRX.BUDGET_TYPE = 'BALANCE' THEN DET_TRX.PERCENTAGE
ELSE NULL
END AS "TRX_%",
CASE WHEN HDR.BUDGET_TYPE = 'POSTING' AND DET_TRX.BUDGET_TYPE = 'BALANCE' THEN DET_TRX.AMOUNT
ELSE NULL
END AS YB_AMT,
NULL AS YB_RMN,
CASE WHEN DET_TRX.BUDGET_TYPE = 'BALANCE' THEN 0
     WHEN DET_TRX.BUDGET_TYPE = 'ADJUSTMENT' THEN DET_TRX.AMOUNT
ELSE NULL
END AS B_To_Date_AMT,
NULL AS B_To_Date_USE,
NULL AS B_Over_AMT,
NULL AS B_To_Date_RMN
FROM
FOCUSPP.BUDGET_CUST_TRAN DET_TRX,
FOCUSPP.BUDGET_CUSTOMER DET,
FOCUSPP.BUDGET_CUST_HDR HDR
WHERE
DET.BUDGET_CUST_HDR_ID = HDR.BUDGET_CUST_HDR_ID
AND DET.BUDGET_CUSTOMER_ID = DET_TRX.BUDGET_CUSTOMER_ID) TBL
ORDER BY YB_TYPE, YB_YEAR, YB_CODE, PCATEGORY_CODE, PCLASS_CODE, PBRAND_CODE, PEXT_CODE, PPACKAGING_CODE, PVARIANT_CODE, NO_URUT, TRX_TYPE, TRX_DATE]]></userDefinedSQL>
