<userDefinedSQL><![CDATA[CREATE OR REPLACE VIEW FCS_RPT_VIEW_MF
AS 
SELECT PromoProduk.PROMO_PRODUK_ID, 
       PromoProduk.PROPOSAL_ID, 
       PromoProduk.PRODUCT_CATEGORY, 
       PromoProduk.PRODUCT_CLASS, 
       PromoProduk.PRODUCT_BRAND, 
       PromoProduk.PRODUCT_EXT, 
       PromoProduk.PRODUCT_PACK, 
       PromoProduk.DESCR, 
       PromoProduk.MEKANISME, 
       PromoProduk.AMOUNT_PROMO, 
       PromoProduk.EST_BUDGET_PROP, 
       PromoProduk.REG_CUST_FLAG, 
       (SELECT LISTAGG(PVAR.SET_VARIANT_DESC, ', ') WITHIN GROUP (ORDER BY PVAR.SET_VARIANT_DESC)  FROM (SELECT PV.PROMO_PRODUK_ID, IMC.SET_VARIANT_DESC FROM PRODUK_VARIANT PV, APPS.FCS_VIEW_ITEM_MASTER_CATEGORY IMC  WHERE IMC.SET_VARIANT = PV.PROD_VARIANT GROUP BY PV.PROMO_PRODUK_ID, IMC.SET_VARIANT_DESC) PVAR WHERE PVAR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID  GROUP BY PVAR.PROMO_PRODUK_ID) AS PRODUCT_VARIANT, 
       (SELECT LISTAGG(PITEM.ITEM_DESCRIPTION, ', ') WITHIN GROUP (ORDER BY PITEM.ITEM_DESCRIPTION)   FROM (SELECT PI.PROMO_PRODUK_ID, IMC.ITEM_DESCRIPTION FROM PRODUK_ITEM PI, APPS.FCS_VIEW_ITEM_MASTER_CATEGORY IMC  WHERE IMC.ITEM = PI.PROD_ITEM GROUP BY PI.PROMO_PRODUK_ID, IMC.ITEM_DESCRIPTION) PITEM  WHERE PITEM.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID  GROUP BY PITEM.PROMO_PRODUK_ID) AS PRODUCT_ITEM, 
       (SELECT SUM(NVL(DIS.DISC_NON_YEARLY,0)) AS NON_YEARLY FROM TARGET TR, DISCOUNT DIS  WHERE TR.PROMO_PRODUK_ID = DIS.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS DISC_NON_YEARLY, 
       (SELECT SUM(NVL(DIS.DISC_YEARLY,0)) AS YEARLY FROM TARGET TR, DISCOUNT DIS  WHERE TR.PROMO_PRODUK_ID = DIS.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS DISC_YEARLY, 
       (SELECT SUM((((NVL(DIS.DISC_NON_YEARLY,0))/NVL(TR.VALUE,1))*100)) AS RASIO_NON_YEARLY FROM TARGET TR, DISCOUNT DIS  WHERE TR.PROMO_PRODUK_ID = DIS.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS DISC_RASIO_NON_YEARLY, 
       (SELECT SUM((((NVL(DIS.DISC_YEARLY,0))/NVL(TR.VALUE,1))*100)) AS RASIO_YEARLY FROM TARGET TR, DISCOUNT DIS  WHERE TR.PROMO_PRODUK_ID = DIS.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS DISC_RASIO_YEARLY, 
       (SELECT SUM(((NVL(DIS.DISC_NON_YEARLY,0) + NVL(DIS.DISC_YEARLY,0))/NVL(TR.VALUE,1)*100)) AS RASIO_TOTAL FROM TARGET TR, DISCOUNT DIS  WHERE TR.PROMO_PRODUK_ID = DIS.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS DISC_RASIO_TOTAL, 
       (SELECT SUM(NVL(BON.DISC_NON_YEARLY,0) * NVL(BON.QTY_FROM,0)) AS NON_YEARLY FROM TARGET TR, PROMO_BONUS BON  WHERE TR.PROMO_PRODUK_ID = BON.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BARANG_BONUS_NON_YEARLY, 
       (SELECT SUM(NVL(BON.DISC_YEARLY,0) * NVL(BON.QTY_FROM,0)) AS YEARLY FROM TARGET TR, PROMO_BONUS BON  WHERE TR.PROMO_PRODUK_ID = BON.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BARANG_BONUS_YEARLY, 
       (SELECT SUM((((NVL(BON.DISC_NON_YEARLY,0))/NVL(TR.VALUE,1))*100)) AS RASIO_NON_YEARLY FROM TARGET TR, PROMO_BONUS BON  WHERE TR.PROMO_PRODUK_ID = BON.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BARANG_BONUS_RASIO_NON_YEARLY, 
       (SELECT SUM((((NVL(BON.DISC_YEARLY,0))/NVL(TR.VALUE,1))*100)) AS RASIO_YEARLY FROM TARGET TR, PROMO_BONUS BON  WHERE TR.PROMO_PRODUK_ID = BON.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BARANG_BONUS_RASIO_YEARLY, 
       (SELECT SUM(((NVL(BON.DISC_NON_YEARLY,0) + NVL(BON.DISC_YEARLY,0))/NVL(TR.VALUE,1)*100)) AS RASIO_TOTAL FROM TARGET TR, PROMO_BONUS BON  WHERE TR.PROMO_PRODUK_ID = BON.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BARANG_BONUS_RASIO_TOTAL, 
       (SELECT SUM(NVL(BIA.BIAYA_NON_YEARLY,0)) AS NON_YEARLY FROM TARGET TR, BIAYA BIA  WHERE TR.PROMO_PRODUK_ID = BIA.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BIAYA_NON_YEARLY, 
       (SELECT SUM(NVL(BIA.BIAYA_YEARLY,0)) AS YEARLY FROM TARGET TR, BIAYA BIA  WHERE TR.PROMO_PRODUK_ID = BIA.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BIAYA_YEARLY, 
       (SELECT SUM((((NVL(BIA.BIAYA_NON_YEARLY,0))/NVL(TR.VALUE,1))*100)) AS RASIO_NON_YEARLY FROM TARGET TR, BIAYA BIA  WHERE TR.PROMO_PRODUK_ID = BIA.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BIAYA_RASIO_NON_YEARLY, 
       (SELECT SUM((((NVL(BIA.BIAYA_YEARLY,0))/NVL(TR.VALUE,1))*100)) AS RASIO_YEARLY FROM TARGET TR, BIAYA BIA  WHERE TR.PROMO_PRODUK_ID = BIA.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BIAYA_RASIO_YEARLY, 
       (SELECT SUM(((NVL(BIA.BIAYA_NON_YEARLY,0) + NVL(BIA.BIAYA_YEARLY,0))/NVL(TR.VALUE,1)*100)) AS RASIO_TOTAL FROM TARGET TR, BIAYA BIA  WHERE TR.PROMO_PRODUK_ID = BIA.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BIAYA_RASIO_TOTAL, 
       PromoProduk.BUDGET_BY, 
       PromoProduk.KODE_POSTING, 
       PromoProduk.VALID_COMB, 
       PromoProduk.ITEM_EXPENSE, 
       PromoProduk.KODE_POSTING_MF, 
       PromoProduk.EXCL_CUST_BY, 
       (SELECT CASE WHEN PR.USER_TYPE_CREATOR = 'HO' THEN 'APP_CUST_EXCL_' || PD.REG_CUST_FLAG WHEN PR.USER_TYPE_CREATOR = 'AREA' THEN 'APP_CUST_EXCL_' || PR.CUST_REG_FLAG ELSE '' END FROM PROPOSAL PR, PROMO_PRODUK PD WHERE PD.PROPOSAL_ID = PR.PROPOSAL_ID AND PD.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS EXCL_CUST_LOV_PARAM, 
       (SELECT LISTAGG(PV.PROD_VARIANT, ',') WITHIN GROUP (ORDER BY PV.PROD_VARIANT) FROM PRODUK_VARIANT PV WHERE PV.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID GROUP BY PV.PROMO_PRODUK_ID) AS PRODUCT_VAR_CODE, 
       (SELECT LISTAGG(PI.PROD_ITEM, ',') WITHIN GROUP (ORDER BY PI.PROD_ITEM) FROM PRODUK_ITEM PI WHERE PI.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID  GROUP BY PI.PROMO_PRODUK_ID) AS PRODUCT_ITEM_CODE, 
       PromoProduk.DISC_MF, 
       PromoProduk.DISC_ON_TOP, 
       PromoProduk.DISC_RASIO_MF, 
       PromoProduk.DISC_RASIO_ON_TOP, 
       PromoProduk.DISC_RASIO_TOTAL AS DISC_RASIO_TOTAL1
FROM PROMO_PRODUK PromoProduk
ORDER BY PromoProduk.PROMO_PRODUK_ID ASC]]></userDefinedSQL>
