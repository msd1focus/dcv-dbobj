<userDefinedSQL>CREATE OR REPLACE VIEW FCS_RPT_VIEW_MF&lt;br/&gt;AS &lt;br/&gt;SELECT PromoProduk.PROMO_PRODUK_ID, &lt;br/&gt;       PromoProduk.PROPOSAL_ID, &lt;br/&gt;       PromoProduk.PRODUCT_CATEGORY, &lt;br/&gt;       PromoProduk.PRODUCT_CLASS, &lt;br/&gt;       PromoProduk.PRODUCT_BRAND, &lt;br/&gt;       PromoProduk.PRODUCT_EXT, &lt;br/&gt;       PromoProduk.PRODUCT_PACK, &lt;br/&gt;       PromoProduk.DESCR, &lt;br/&gt;       PromoProduk.MEKANISME, &lt;br/&gt;       PromoProduk.AMOUNT_PROMO, &lt;br/&gt;       PromoProduk.EST_BUDGET_PROP, &lt;br/&gt;       PromoProduk.REG_CUST_FLAG, &lt;br/&gt;       (SELECT LISTAGG(PVAR.SET_VARIANT_DESC, &apos;, &apos;) WITHIN GROUP (ORDER BY PVAR.SET_VARIANT_DESC)  FROM (SELECT PV.PROMO_PRODUK_ID, IMC.SET_VARIANT_DESC FROM PRODUK_VARIANT PV, APPS.FCS_VIEW_ITEM_MASTER_CATEGORY IMC  WHERE IMC.SET_VARIANT = PV.PROD_VARIANT GROUP BY PV.PROMO_PRODUK_ID, IMC.SET_VARIANT_DESC) PVAR WHERE PVAR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID  GROUP BY PVAR.PROMO_PRODUK_ID) AS PRODUCT_VARIANT, &lt;br/&gt;       (SELECT LISTAGG(PITEM.ITEM_DESCRIPTION, &apos;, &apos;) WITHIN GROUP (ORDER BY PITEM.ITEM_DESCRIPTION)   FROM (SELECT PI.PROMO_PRODUK_ID, IMC.ITEM_DESCRIPTION FROM PRODUK_ITEM PI, APPS.FCS_VIEW_ITEM_MASTER_CATEGORY IMC  WHERE IMC.ITEM = PI.PROD_ITEM GROUP BY PI.PROMO_PRODUK_ID, IMC.ITEM_DESCRIPTION) PITEM  WHERE PITEM.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID  GROUP BY PITEM.PROMO_PRODUK_ID) AS PRODUCT_ITEM, &lt;br/&gt;       (SELECT SUM(NVL(DIS.DISC_NON_YEARLY,0)) AS NON_YEARLY FROM TARGET TR, DISCOUNT DIS  WHERE TR.PROMO_PRODUK_ID = DIS.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS DISC_NON_YEARLY, &lt;br/&gt;       (SELECT SUM(NVL(DIS.DISC_YEARLY,0)) AS YEARLY FROM TARGET TR, DISCOUNT DIS  WHERE TR.PROMO_PRODUK_ID = DIS.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS DISC_YEARLY, &lt;br/&gt;       (SELECT SUM((((NVL(DIS.DISC_NON_YEARLY,0))/NVL(TR.VALUE,1))*100)) AS RASIO_NON_YEARLY FROM TARGET TR, DISCOUNT DIS  WHERE TR.PROMO_PRODUK_ID = DIS.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS DISC_RASIO_NON_YEARLY, &lt;br/&gt;       (SELECT SUM((((NVL(DIS.DISC_YEARLY,0))/NVL(TR.VALUE,1))*100)) AS RASIO_YEARLY FROM TARGET TR, DISCOUNT DIS  WHERE TR.PROMO_PRODUK_ID = DIS.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS DISC_RASIO_YEARLY, &lt;br/&gt;       (SELECT SUM(((NVL(DIS.DISC_NON_YEARLY,0) + NVL(DIS.DISC_YEARLY,0))/NVL(TR.VALUE,1)*100)) AS RASIO_TOTAL FROM TARGET TR, DISCOUNT DIS  WHERE TR.PROMO_PRODUK_ID = DIS.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS DISC_RASIO_TOTAL, &lt;br/&gt;       (SELECT SUM(NVL(BON.DISC_NON_YEARLY,0) * NVL(BON.QTY_FROM,0)) AS NON_YEARLY FROM TARGET TR, PROMO_BONUS BON  WHERE TR.PROMO_PRODUK_ID = BON.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BARANG_BONUS_NON_YEARLY, &lt;br/&gt;       (SELECT SUM(NVL(BON.DISC_YEARLY,0) * NVL(BON.QTY_FROM,0)) AS YEARLY FROM TARGET TR, PROMO_BONUS BON  WHERE TR.PROMO_PRODUK_ID = BON.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BARANG_BONUS_YEARLY, &lt;br/&gt;       (SELECT SUM((((NVL(BON.DISC_NON_YEARLY,0))/NVL(TR.VALUE,1))*100)) AS RASIO_NON_YEARLY FROM TARGET TR, PROMO_BONUS BON  WHERE TR.PROMO_PRODUK_ID = BON.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BARANG_BONUS_RASIO_NON_YEARLY, &lt;br/&gt;       (SELECT SUM((((NVL(BON.DISC_YEARLY,0))/NVL(TR.VALUE,1))*100)) AS RASIO_YEARLY FROM TARGET TR, PROMO_BONUS BON  WHERE TR.PROMO_PRODUK_ID = BON.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BARANG_BONUS_RASIO_YEARLY, &lt;br/&gt;       (SELECT SUM(((NVL(BON.DISC_NON_YEARLY,0) + NVL(BON.DISC_YEARLY,0))/NVL(TR.VALUE,1)*100)) AS RASIO_TOTAL FROM TARGET TR, PROMO_BONUS BON  WHERE TR.PROMO_PRODUK_ID = BON.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BARANG_BONUS_RASIO_TOTAL, &lt;br/&gt;       (SELECT SUM(NVL(BIA.BIAYA_NON_YEARLY,0)) AS NON_YEARLY FROM TARGET TR, BIAYA BIA  WHERE TR.PROMO_PRODUK_ID = BIA.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BIAYA_NON_YEARLY, &lt;br/&gt;       (SELECT SUM(NVL(BIA.BIAYA_YEARLY,0)) AS YEARLY FROM TARGET TR, BIAYA BIA  WHERE TR.PROMO_PRODUK_ID = BIA.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BIAYA_YEARLY, &lt;br/&gt;       (SELECT SUM((((NVL(BIA.BIAYA_NON_YEARLY,0))/NVL(TR.VALUE,1))*100)) AS RASIO_NON_YEARLY FROM TARGET TR, BIAYA BIA  WHERE TR.PROMO_PRODUK_ID = BIA.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BIAYA_RASIO_NON_YEARLY, &lt;br/&gt;       (SELECT SUM((((NVL(BIA.BIAYA_YEARLY,0))/NVL(TR.VALUE,1))*100)) AS RASIO_YEARLY FROM TARGET TR, BIAYA BIA  WHERE TR.PROMO_PRODUK_ID = BIA.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BIAYA_RASIO_YEARLY, &lt;br/&gt;       (SELECT SUM(((NVL(BIA.BIAYA_NON_YEARLY,0) + NVL(BIA.BIAYA_YEARLY,0))/NVL(TR.VALUE,1)*100)) AS RASIO_TOTAL FROM TARGET TR, BIAYA BIA  WHERE TR.PROMO_PRODUK_ID = BIA.PROMO_PRODUK_ID AND TR.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS BIAYA_RASIO_TOTAL, &lt;br/&gt;       PromoProduk.BUDGET_BY, &lt;br/&gt;       PromoProduk.KODE_POSTING, &lt;br/&gt;       PromoProduk.VALID_COMB, &lt;br/&gt;       PromoProduk.ITEM_EXPENSE, &lt;br/&gt;       PromoProduk.KODE_POSTING_MF, &lt;br/&gt;       PromoProduk.EXCL_CUST_BY, &lt;br/&gt;       (SELECT CASE WHEN PR.USER_TYPE_CREATOR = &apos;HO&apos; THEN &apos;APP_CUST_EXCL_&apos; || PD.REG_CUST_FLAG WHEN PR.USER_TYPE_CREATOR = &apos;AREA&apos; THEN &apos;APP_CUST_EXCL_&apos; || PR.CUST_REG_FLAG ELSE &apos;&apos; END FROM PROPOSAL PR, PROMO_PRODUK PD WHERE PD.PROPOSAL_ID = PR.PROPOSAL_ID AND PD.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID) AS EXCL_CUST_LOV_PARAM, &lt;br/&gt;       (SELECT LISTAGG(PV.PROD_VARIANT, &apos;,&apos;) WITHIN GROUP (ORDER BY PV.PROD_VARIANT) FROM PRODUK_VARIANT PV WHERE PV.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID GROUP BY PV.PROMO_PRODUK_ID) AS PRODUCT_VAR_CODE, &lt;br/&gt;       (SELECT LISTAGG(PI.PROD_ITEM, &apos;,&apos;) WITHIN GROUP (ORDER BY PI.PROD_ITEM) FROM PRODUK_ITEM PI WHERE PI.PROMO_PRODUK_ID = PromoProduk.PROMO_PRODUK_ID  GROUP BY PI.PROMO_PRODUK_ID) AS PRODUCT_ITEM_CODE, &lt;br/&gt;       PromoProduk.DISC_MF, &lt;br/&gt;       PromoProduk.DISC_ON_TOP, &lt;br/&gt;       PromoProduk.DISC_RASIO_MF, &lt;br/&gt;       PromoProduk.DISC_RASIO_ON_TOP, &lt;br/&gt;       PromoProduk.DISC_RASIO_TOTAL AS DISC_RASIO_TOTAL1&lt;br/&gt;FROM PROMO_PRODUK PromoProduk&lt;br/&gt;ORDER BY PromoProduk.PROMO_PRODUK_ID ASC</userDefinedSQL>
