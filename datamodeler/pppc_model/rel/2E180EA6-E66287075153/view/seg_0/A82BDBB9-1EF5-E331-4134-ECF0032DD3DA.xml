<TableView class="oracle.dbtools.crest.model.design.relational.TableView" directorySegmentName="seg_0" id="A82BDBB9-1EF5-E331-4134-ECF0032DD3DA" schemaObject="7CBB1AB6-5E3C-CEFF-688A-822A0E866097" name="FCS_SHUTTLE_USER_AREA">
<sourceConnName>pppc@uatfdi</sourceConnName>
<sourceObjSchema>FOCUSPP</sourceObjSchema>
<sourceObjName>FCS_SHUTTLE_USER_AREA</sourceObjName>
