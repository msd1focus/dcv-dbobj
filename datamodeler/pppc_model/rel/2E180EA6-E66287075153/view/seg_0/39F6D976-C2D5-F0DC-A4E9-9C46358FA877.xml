<userDefinedSQL>CREATE OR REPLACE VIEW FCS_YEARLY_BUDGET_DETAIL_V&lt;br/&gt;AS &lt;br/&gt;SELECT TBL.&quot;NO_URUT&quot;,TBL.&quot;YB_ID&quot;,TBL.&quot;YB_TYPE&quot;,TBL.&quot;YB_YEAR&quot;,TBL.&quot;YB_CODE&quot;,TBL.&quot;YB_DESC&quot;,TBL.&quot;PCATEGORY_CODE&quot;,TBL.&quot;PCATEGORY_DESC&quot;,TBL.&quot;PCLASS_CODE&quot;,TBL.&quot;PCLASS_DESC&quot;,TBL.&quot;PBRAND_CODE&quot;,TBL.&quot;PBRAND_DESC&quot;,TBL.&quot;PEXT_CODE&quot;,TBL.&quot;PEXT_DESC&quot;,TBL.&quot;PPACKAGING_CODE&quot;,TBL.&quot;PPACKAGING_DESC&quot;,TBL.&quot;PVARIANT_CODE&quot;,TBL.&quot;PVARIANT_DESC&quot;,TBL.&quot;STATUS&quot;,TBL.&quot;TRX_TYPE&quot;,TBL.&quot;TRX_DATE&quot;,TBL.&quot;TRX_REMARKS&quot;,TBL.&quot;TRX_CREATE_BY&quot;,TBL.&quot;TRX_CREATE_DATE&quot;,TBL.&quot;TRX_MODIFIED_BY&quot;,TBL.&quot;TRX_MODIFIED_DATE&quot;,TBL.&quot;TRX_ACTION&quot;,TBL.&quot;TRX_ACTION_BY&quot;,TBL.&quot;TRX_ACTION_DATE&quot;,TBL.&quot;TRX_AMOUNT&quot;,TBL.&quot;TRX_%&quot;,TBL.&quot;YB_AMT&quot;,TBL.&quot;YB_RMN&quot;,TBL.&quot;B_TO_DATE_AMT&quot;,TBL.&quot;B_TO_DATE_USE&quot;,TBL.&quot;B_OVER_AMT&quot;,TBL.&quot;B_TO_DATE_RMN&quot; &lt;br/&gt;FROM&lt;br/&gt;(SELECT &lt;br/&gt;&apos;001&apos; AS No_URUT,&lt;br/&gt;HDR.BUDGET_CUST_HDR_ID YB_ID,&lt;br/&gt;HDR.BUDGET_TYPE YB_TYPE,&lt;br/&gt;HDR.BUDGET_YEAR YB_YEAR,&lt;br/&gt;CASE WHEN HDR.BUDGET_TYPE = &apos;CUSTOMER&apos; THEN HDR.CUSTOMER_ID&lt;br/&gt;     WHEN HDR.BUDGET_TYPE = &apos;POSTING&apos; THEN HDR.KODE_POSTING&lt;br/&gt;ELSE NULL&lt;br/&gt;END AS YB_CODE,&lt;br/&gt;CASE WHEN HDR.BUDGET_TYPE = &apos;CUSTOMER&apos; THEN&lt;br/&gt;          (SELECT ffv.description&lt;br/&gt;             FROM apps.fnd_flex_value_sets ffvs, apps.fnd_flex_values_vl ffv&lt;br/&gt;            WHERE ffv.flex_value_set_id = ffvs.flex_value_set_id&lt;br/&gt;              AND ffvs.flex_value_set_name = &apos;FCSAR_CUSTOMER_GROUP&apos;&lt;br/&gt;              AND ffv.flex_value = HDR.CUSTOMER_ID) &lt;br/&gt;     WHEN HDR.BUDGET_TYPE = &apos;POSTING&apos; THEN&lt;br/&gt;          (SELECT msi.description&lt;br/&gt;             FROM apps.fcs_inv_view_mstr_item msi&lt;br/&gt;            WHERE msi.item_type = &apos;PST&apos;&lt;br/&gt;              AND msi.item = HDR.KODE_POSTING)&lt;br/&gt;ELSE NULL&lt;br/&gt;END AS YB_DESC,&lt;br/&gt;DET.BUDGET_CATEGORY PCATEGORY_CODE,&lt;br/&gt;(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_CATEGORY AND MCV.STRUCTURE_NAME = &apos;FCS_CATEGORY&apos;) AS PCATEGORY_DESC,&lt;br/&gt;DET.BUDGET_CLASS PCLASS_CODE,&lt;br/&gt;(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_CLASS AND MCV.STRUCTURE_NAME = &apos;FCS_CLASS&apos;) AS PCLASS_DESC,&lt;br/&gt;DET.BUDGET_BRAND PBRAND_CODE,&lt;br/&gt;(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_BRAND AND MCV.STRUCTURE_NAME = &apos;FCS_BRAND&apos;) AS PBRAND_DESC,&lt;br/&gt;DET.BUDGET_EXTENTION PEXT_CODE,&lt;br/&gt;(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_EXTENTION AND MCV.STRUCTURE_NAME = &apos;FCS_EXTENTION&apos;) AS PEXT_DESC,&lt;br/&gt;DET.BUDGET_PACKAGING PPACKAGING_CODE,&lt;br/&gt;(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_PACKAGING AND MCV.STRUCTURE_NAME = &apos;FCS_PACKAGING&apos;) AS PPACKAGING_DESC,&lt;br/&gt;DET.BUDGET_VARIANT PVARIANT_CODE,&lt;br/&gt;(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_VARIANT AND MCV.STRUCTURE_NAME = &apos;FCS_VARIANT&apos;) AS PVARIANT_DESC,&lt;br/&gt;DET.STATUS,&lt;br/&gt;NULL AS TRX_TYPE,&lt;br/&gt;NULL AS TRX_DATE,&lt;br/&gt;NULL AS TRX_REMARKS,&lt;br/&gt;NULL AS TRX_CREATE_BY,&lt;br/&gt;NULL AS TRX_CREATE_DATE,&lt;br/&gt;NULL AS TRX_MODIFIED_BY,&lt;br/&gt;NULL AS TRX_MODIFIED_DATE,&lt;br/&gt;NULL AS TRX_ACTION,&lt;br/&gt;NULL AS TRX_ACTION_BY,&lt;br/&gt;NULL AS TRX_ACTION_DATE,&lt;br/&gt;NULL AS TRX_AMOUNT,&lt;br/&gt;NULL AS &quot;TRX_%&quot;,&lt;br/&gt;DET.YEARLY_BUDGET_AMOUNT YB_AMT,&lt;br/&gt;DET.YEARLY_BUDGET_AMOUNT - DET.BUDGET_AS_TO_DATE YB_RMN,&lt;br/&gt;DET.BUDGET_AS_TO_DATE B_To_Date_AMT,&lt;br/&gt;DET.BUDGET_AS_TO_DATE_USED B_To_Date_USE,&lt;br/&gt;DET.OVER_BUDGET B_Over_AMT,&lt;br/&gt;DET.BUDGET_AS_TO_DATE - DET.BUDGET_AS_TO_DATE_USED - (CASE WHEN DET.OVER_BUDGET IS NULL THEN 0 ELSE DET.OVER_BUDGET END) B_To_Date_Rmn&lt;br/&gt;FROM&lt;br/&gt;FOCUSPP.BUDGET_CUSTOMER DET,&lt;br/&gt;FOCUSPP.BUDGET_CUST_HDR HDR&lt;br/&gt;WHERE&lt;br/&gt;DET.BUDGET_CUST_HDR_ID = HDR.BUDGET_CUST_HDR_ID&lt;br/&gt;UNION ALL&lt;br/&gt;SELECT&lt;br/&gt;&apos;002&apos; AS No_URUT,&lt;br/&gt;DET_TRX_YB.YB_ID,&lt;br/&gt;DET_TRX_YB.YB_TYPE,&lt;br/&gt;DET_TRX_YB.YB_YEAR,&lt;br/&gt;DET_TRX_YB.YB_CODE,&lt;br/&gt;DET_TRX_YB.YB_DESC,&lt;br/&gt;DET_TRX_YB.PCATEGORY_CODE,&lt;br/&gt;DET_TRX_YB.PCATEGORY_DESC,&lt;br/&gt;DET_TRX_YB.PCLASS_CODE,&lt;br/&gt;DET_TRX_YB.PCLASS_DESC,&lt;br/&gt;DET_TRX_YB.PBRAND_CODE,&lt;br/&gt;DET_TRX_YB.PBRAND_DESC,&lt;br/&gt;DET_TRX_YB.PEXT_CODE,&lt;br/&gt;DET_TRX_YB.PEXT_DESC,&lt;br/&gt;DET_TRX_YB.PPACKAGING_CODE,&lt;br/&gt;DET_TRX_YB.PPACKAGING_DESC,&lt;br/&gt;DET_TRX_YB.PVARIANT_CODE,&lt;br/&gt;DET_TRX_YB.PVARIANT_DESC,&lt;br/&gt;DET_TRX_YB.STATUS,&lt;br/&gt;NULL AS TRX_TYPE,&lt;br/&gt;NULL AS TRX_DATE,&lt;br/&gt;NULL AS TRX_REMARKS,&lt;br/&gt;NULL AS TRX_CREATE_BY,&lt;br/&gt;NULL AS TRX_CREATE_DATE,&lt;br/&gt;NULL AS TRX_MODIFIED_BY,&lt;br/&gt;NULL AS TRX_MODIFIED_DATE,&lt;br/&gt;NULL AS TRX_ACTION,&lt;br/&gt;NULL AS TRX_ACTION_BY,&lt;br/&gt;NULL AS TRX_ACTION_DATE,&lt;br/&gt;CASE WHEN DET_TRX_YB.YB_TYPE = &apos;CUSTOMER&apos; THEN SUM(DET_TRX_YB.TRX_AMOUNT)&lt;br/&gt;ELSE NULL&lt;br/&gt;END AS TRX_AMOUNT,&lt;br/&gt;CASE WHEN DET_TRX_YB.YB_TYPE = &apos;CUSTOMER&apos; THEN SUM(DET_TRX_YB.&quot;TRX_%&quot;)&lt;br/&gt;ELSE NULL&lt;br/&gt;END AS &quot;TRX_%&quot;,&lt;br/&gt;CASE WHEN DET_TRX_YB.YB_TYPE = &apos;CUSTOMER&apos; THEN SUM(DET_TRX_YB.TRX_AMOUNT) * SUM(DET_TRX_YB.&quot;TRX_%&quot;) / 100&lt;br/&gt;     WHEN DET_TRX_YB.YB_TYPE = &apos;POSTING&apos; THEN SUM(DET_TRX_YB.YB_AMT)&lt;br/&gt;ELSE NULL&lt;br/&gt;END AS YB_AMT,&lt;br/&gt;CASE WHEN DET_TRX_YB.YB_TYPE = &apos;CUSTOMER&apos; THEN (SUM(DET_TRX_YB.TRX_AMOUNT) * SUM(DET_TRX_YB.&quot;TRX_%&quot;) / 100) - SUM(DET_TRX_YB.B_To_Date_AMT)&lt;br/&gt;     WHEN DET_TRX_YB.YB_TYPE = &apos;POSTING&apos; THEN SUM(DET_TRX_YB.YB_AMT) - SUM(DET_TRX_YB.B_To_Date_AMT)&lt;br/&gt;ELSE NULL&lt;br/&gt;END AS YB_RMN,&lt;br/&gt;SUM(DET_TRX_YB.B_To_Date_AMT) B_To_Date_AMT,&lt;br/&gt;NULL AS B_To_Date_USE,&lt;br/&gt;NULL AS B_Over_AMT,&lt;br/&gt;NULL AS B_To_Date_RMN&lt;br/&gt;FROM&lt;br/&gt;(SELECT&lt;br/&gt;HDR.BUDGET_CUST_HDR_ID YB_ID,&lt;br/&gt;HDR.BUDGET_TYPE YB_TYPE,&lt;br/&gt;HDR.BUDGET_YEAR YB_YEAR,&lt;br/&gt;CASE WHEN HDR.BUDGET_TYPE = &apos;CUSTOMER&apos; THEN HDR.CUSTOMER_ID&lt;br/&gt;     WHEN HDR.BUDGET_TYPE = &apos;POSTING&apos; THEN HDR.KODE_POSTING&lt;br/&gt;ELSE NULL&lt;br/&gt;END AS YB_CODE,&lt;br/&gt;CASE WHEN HDR.BUDGET_TYPE = &apos;CUSTOMER&apos; THEN&lt;br/&gt;          (SELECT ffv.description&lt;br/&gt;             FROM apps.fnd_flex_value_sets ffvs, apps.fnd_flex_values_vl ffv&lt;br/&gt;            WHERE ffv.flex_value_set_id = ffvs.flex_value_set_id&lt;br/&gt;              AND ffvs.flex_value_set_name = &apos;FCSAR_CUSTOMER_GROUP&apos;&lt;br/&gt;              AND ffv.flex_value = HDR.CUSTOMER_ID) &lt;br/&gt;     WHEN HDR.BUDGET_TYPE = &apos;POSTING&apos; THEN&lt;br/&gt;          (SELECT msi.description&lt;br/&gt;             FROM apps.fcs_inv_view_mstr_item msi&lt;br/&gt;            WHERE msi.item_type = &apos;PST&apos;&lt;br/&gt;              AND msi.item = HDR.KODE_POSTING)&lt;br/&gt;ELSE NULL&lt;br/&gt;END AS YB_DESC,&lt;br/&gt;DET.BUDGET_CATEGORY PCATEGORY_CODE,&lt;br/&gt;(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_CATEGORY AND MCV.STRUCTURE_NAME = &apos;FCS_CATEGORY&apos;) AS PCATEGORY_DESC,&lt;br/&gt;DET.BUDGET_CLASS PCLASS_CODE,&lt;br/&gt;(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_CLASS AND MCV.STRUCTURE_NAME = &apos;FCS_CLASS&apos;) AS PCLASS_DESC,&lt;br/&gt;DET.BUDGET_BRAND PBRAND_CODE,&lt;br/&gt;(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_BRAND AND MCV.STRUCTURE_NAME = &apos;FCS_BRAND&apos;) AS PBRAND_DESC,&lt;br/&gt;DET.BUDGET_EXTENTION PEXT_CODE,&lt;br/&gt;(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_EXTENTION AND MCV.STRUCTURE_NAME = &apos;FCS_EXTENTION&apos;) AS PEXT_DESC,&lt;br/&gt;DET.BUDGET_PACKAGING PPACKAGING_CODE,&lt;br/&gt;(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_PACKAGING AND MCV.STRUCTURE_NAME = &apos;FCS_PACKAGING&apos;) AS PPACKAGING_DESC,&lt;br/&gt;DET.BUDGET_VARIANT PVARIANT_CODE,&lt;br/&gt;(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_VARIANT AND MCV.STRUCTURE_NAME = &apos;FCS_VARIANT&apos;) AS PVARIANT_DESC,&lt;br/&gt;DET.STATUS,&lt;br/&gt;DET_TRX.BUDGET_TYPE TRX_TYPE,&lt;br/&gt;DET_TRX.BUDGET_DATE TRX_DATE,&lt;br/&gt;DET_TRX.REMARKS TRX_REMARKS,&lt;br/&gt;DET_TRX.CREATED_BY TRX_CREATE_BY,&lt;br/&gt;DET_TRX.CREATION_DATE TRX_CREATE_DATE,&lt;br/&gt;DET_TRX.MODIFIED_BY TRX_MODIFIED_BY,&lt;br/&gt;DET_TRX.MODIFIED_DATE TRX_MODIFIED_DATE,&lt;br/&gt;DET_TRX.ACTION TRX_ACTION,&lt;br/&gt;DET_TRX.ACTION_BY TRX_ACTION_BY,&lt;br/&gt;DET_TRX.ACTION_DATE TRX_ACTION_DATE,&lt;br/&gt;CASE WHEN HDR.BUDGET_TYPE = &apos;CUSTOMER&apos; AND DET_TRX.BUDGET_TYPE = &apos;BALANCE&apos; THEN DET_TRX.AMOUNT&lt;br/&gt;ELSE NULL&lt;br/&gt;END AS TRX_AMOUNT,&lt;br/&gt;CASE WHEN HDR.BUDGET_TYPE = &apos;CUSTOMER&apos; AND DET_TRX.BUDGET_TYPE = &apos;BALANCE&apos; THEN DET_TRX.PERCENTAGE&lt;br/&gt;ELSE NULL&lt;br/&gt;END AS &quot;TRX_%&quot;,&lt;br/&gt;CASE WHEN HDR.BUDGET_TYPE = &apos;POSTING&apos; AND DET_TRX.BUDGET_TYPE = &apos;BALANCE&apos; THEN DET_TRX.AMOUNT&lt;br/&gt;ELSE NULL&lt;br/&gt;END AS YB_AMT,&lt;br/&gt;NULL AS YB_RMN,&lt;br/&gt;CASE WHEN DET_TRX.BUDGET_TYPE = &apos;BALANCE&apos; THEN 0&lt;br/&gt;     WHEN DET_TRX.BUDGET_TYPE = &apos;ADJUSTMENT&apos; THEN DET_TRX.AMOUNT&lt;br/&gt;ELSE NULL&lt;br/&gt;END AS B_To_Date_AMT,&lt;br/&gt;NULL AS B_To_Date_USE,&lt;br/&gt;NULL AS B_Over_AMT,&lt;br/&gt;NULL AS B_To_Date_RMN&lt;br/&gt;FROM&lt;br/&gt;FOCUSPP.BUDGET_CUST_TRAN DET_TRX,&lt;br/&gt;FOCUSPP.BUDGET_CUSTOMER DET,&lt;br/&gt;FOCUSPP.BUDGET_CUST_HDR HDR&lt;br/&gt;WHERE&lt;br/&gt;DET.BUDGET_CUST_HDR_ID = HDR.BUDGET_CUST_HDR_ID&lt;br/&gt;AND DET.BUDGET_CUSTOMER_ID = DET_TRX.BUDGET_CUSTOMER_ID) DET_TRX_YB&lt;br/&gt;WHERE &lt;br/&gt;DET_TRX_YB.TRX_ACTION = &apos;APPROVED&apos;&lt;br/&gt;GROUP BY&lt;br/&gt;DET_TRX_YB.YB_ID,&lt;br/&gt;DET_TRX_YB.YB_TYPE,&lt;br/&gt;DET_TRX_YB.YB_YEAR,&lt;br/&gt;DET_TRX_YB.YB_CODE,&lt;br/&gt;DET_TRX_YB.YB_DESC,&lt;br/&gt;DET_TRX_YB.PCATEGORY_CODE,&lt;br/&gt;DET_TRX_YB.PCATEGORY_DESC,&lt;br/&gt;DET_TRX_YB.PCLASS_CODE,&lt;br/&gt;DET_TRX_YB.PCLASS_DESC,&lt;br/&gt;DET_TRX_YB.PBRAND_CODE,&lt;br/&gt;DET_TRX_YB.PBRAND_DESC,&lt;br/&gt;DET_TRX_YB.PEXT_CODE,&lt;br/&gt;DET_TRX_YB.PEXT_DESC,&lt;br/&gt;DET_TRX_YB.PPACKAGING_CODE,&lt;br/&gt;DET_TRX_YB.PPACKAGING_DESC,&lt;br/&gt;DET_TRX_YB.PVARIANT_CODE,&lt;br/&gt;DET_TRX_YB.PVARIANT_DESC,&lt;br/&gt;DET_TRX_YB.STATUS&lt;br/&gt;UNION ALL&lt;br/&gt;SELECT&lt;br/&gt;&apos;003&apos; AS No_URUT,&lt;br/&gt;HDR.BUDGET_CUST_HDR_ID YB_ID,&lt;br/&gt;HDR.BUDGET_TYPE YB_TYPE,&lt;br/&gt;HDR.BUDGET_YEAR YB_YEAR,&lt;br/&gt;CASE WHEN HDR.BUDGET_TYPE = &apos;CUSTOMER&apos; THEN HDR.CUSTOMER_ID&lt;br/&gt;     WHEN HDR.BUDGET_TYPE = &apos;POSTING&apos; THEN HDR.KODE_POSTING&lt;br/&gt;ELSE NULL&lt;br/&gt;END AS YB_CODE,&lt;br/&gt;CASE WHEN HDR.BUDGET_TYPE = &apos;CUSTOMER&apos; THEN&lt;br/&gt;          (SELECT ffv.description&lt;br/&gt;             FROM apps.fnd_flex_value_sets ffvs, apps.fnd_flex_values_vl ffv&lt;br/&gt;            WHERE ffv.flex_value_set_id = ffvs.flex_value_set_id&lt;br/&gt;              AND ffvs.flex_value_set_name = &apos;FCSAR_CUSTOMER_GROUP&apos;&lt;br/&gt;              AND ffv.flex_value = HDR.CUSTOMER_ID) &lt;br/&gt;     WHEN HDR.BUDGET_TYPE = &apos;POSTING&apos; THEN&lt;br/&gt;          (SELECT msi.description&lt;br/&gt;             FROM apps.fcs_inv_view_mstr_item msi&lt;br/&gt;            WHERE msi.item_type = &apos;PST&apos;&lt;br/&gt;              AND msi.item = HDR.KODE_POSTING)&lt;br/&gt;ELSE NULL&lt;br/&gt;END AS YB_DESC,&lt;br/&gt;DET.BUDGET_CATEGORY PCATEGORY_CODE,&lt;br/&gt;(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_CATEGORY AND MCV.STRUCTURE_NAME = &apos;FCS_CATEGORY&apos;) AS PCATEGORY_DESC,&lt;br/&gt;DET.BUDGET_CLASS PCLASS_CODE,&lt;br/&gt;(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_CLASS AND MCV.STRUCTURE_NAME = &apos;FCS_CLASS&apos;) AS PCLASS_DESC,&lt;br/&gt;DET.BUDGET_BRAND PBRAND_CODE,&lt;br/&gt;(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_BRAND AND MCV.STRUCTURE_NAME = &apos;FCS_BRAND&apos;) AS PBRAND_DESC,&lt;br/&gt;DET.BUDGET_EXTENTION PEXT_CODE,&lt;br/&gt;(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_EXTENTION AND MCV.STRUCTURE_NAME = &apos;FCS_EXTENTION&apos;) AS PEXT_DESC,&lt;br/&gt;DET.BUDGET_PACKAGING PPACKAGING_CODE,&lt;br/&gt;(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_PACKAGING AND MCV.STRUCTURE_NAME = &apos;FCS_PACKAGING&apos;) AS PPACKAGING_DESC,&lt;br/&gt;DET.BUDGET_VARIANT PVARIANT_CODE,&lt;br/&gt;(SELECT MCV.DESCRIPTION FROM APPS.MTL_CATEGORIES_V MCV WHERE MCV.CATEGORY_CONCAT_SEGS = DET.BUDGET_VARIANT AND MCV.STRUCTURE_NAME = &apos;FCS_VARIANT&apos;) AS PVARIANT_DESC,&lt;br/&gt;DET.STATUS,&lt;br/&gt;DET_TRX.BUDGET_TYPE TRX_TYPE,&lt;br/&gt;DET_TRX.BUDGET_DATE TRX_DATE,&lt;br/&gt;DET_TRX.REMARKS TRX_REMARKS,&lt;br/&gt;DET_TRX.CREATED_BY TRX_CREATE_BY,&lt;br/&gt;DET_TRX.CREATION_DATE TRX_CREATE_DATE,&lt;br/&gt;DET_TRX.MODIFIED_BY TRX_MODIFIED_BY,&lt;br/&gt;DET_TRX.MODIFIED_DATE TRX_MODIFIED_DATE,&lt;br/&gt;DET_TRX.ACTION TRX_ACTION,&lt;br/&gt;DET_TRX.ACTION_BY TRX_ACTION_BY,&lt;br/&gt;DET_TRX.ACTION_DATE TRX_ACTION_DATE,&lt;br/&gt;CASE WHEN HDR.BUDGET_TYPE = &apos;CUSTOMER&apos; AND DET_TRX.BUDGET_TYPE = &apos;BALANCE&apos; THEN DET_TRX.AMOUNT&lt;br/&gt;ELSE NULL&lt;br/&gt;END AS TRX_AMOUNT,&lt;br/&gt;CASE WHEN HDR.BUDGET_TYPE = &apos;CUSTOMER&apos; AND DET_TRX.BUDGET_TYPE = &apos;BALANCE&apos; THEN DET_TRX.PERCENTAGE&lt;br/&gt;ELSE NULL&lt;br/&gt;END AS &quot;TRX_%&quot;,&lt;br/&gt;CASE WHEN HDR.BUDGET_TYPE = &apos;POSTING&apos; AND DET_TRX.BUDGET_TYPE = &apos;BALANCE&apos; THEN DET_TRX.AMOUNT&lt;br/&gt;ELSE NULL&lt;br/&gt;END AS YB_AMT,&lt;br/&gt;NULL AS YB_RMN,&lt;br/&gt;CASE WHEN DET_TRX.BUDGET_TYPE = &apos;BALANCE&apos; THEN 0&lt;br/&gt;     WHEN DET_TRX.BUDGET_TYPE = &apos;ADJUSTMENT&apos; THEN DET_TRX.AMOUNT&lt;br/&gt;ELSE NULL&lt;br/&gt;END AS B_To_Date_AMT,&lt;br/&gt;NULL AS B_To_Date_USE,&lt;br/&gt;NULL AS B_Over_AMT,&lt;br/&gt;NULL AS B_To_Date_RMN&lt;br/&gt;FROM&lt;br/&gt;FOCUSPP.BUDGET_CUST_TRAN DET_TRX,&lt;br/&gt;FOCUSPP.BUDGET_CUSTOMER DET,&lt;br/&gt;FOCUSPP.BUDGET_CUST_HDR HDR&lt;br/&gt;WHERE&lt;br/&gt;DET.BUDGET_CUST_HDR_ID = HDR.BUDGET_CUST_HDR_ID&lt;br/&gt;AND DET.BUDGET_CUSTOMER_ID = DET_TRX.BUDGET_CUSTOMER_ID) TBL&lt;br/&gt;ORDER BY YB_TYPE, YB_YEAR, YB_CODE, PCATEGORY_CODE, PCLASS_CODE, PBRAND_CODE, PEXT_CODE, PPACKAGING_CODE, PVARIANT_CODE, NO_URUT, TRX_TYPE, TRX_DATE</userDefinedSQL>
