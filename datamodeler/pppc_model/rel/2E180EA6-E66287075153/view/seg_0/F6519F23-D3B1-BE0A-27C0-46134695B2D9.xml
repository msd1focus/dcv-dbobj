<userDefinedSQL>CREATE OR REPLACE VIEW FCS_EXCL_SHUTTLE_AREA_PROP&lt;br/&gt;AS &lt;br/&gt;select arc.attribute4 AS area_code,&lt;br/&gt;arc.attribute4 AS description,&lt;br/&gt;arc.attribute4 || &apos; &apos; || flex.description AS area_label,&lt;br/&gt;aur.user_name AS user_name,           &lt;br/&gt;arc.attribute1 AS cust_group_code,&lt;br/&gt;arc.attribute3 AS region_code,&lt;br/&gt;prod.promo_produk_id AS promo_produk_id,&lt;br/&gt;null AS proposal_id&lt;br/&gt;from apps.ar_customers arc, app_user_region aur, PROP_REGION_CUST_GROUP rcg, PROMO_PRODUK prod, APPS.FCS_FLEX_VALUES_VL flex&lt;br/&gt;where arc.attribute1 = rcg.cust_group&lt;br/&gt;and aur.region_code = arc.attribute3&lt;br/&gt;and arc.status = &apos;A&apos;&lt;br/&gt;and prod.proposal_id = rcg.proposal_id&lt;br/&gt;and flex.flex_value = arc.attribute4&lt;br/&gt;group by arc.attribute4, arc.attribute1, arc.attribute3, aur.user_name, prod.promo_produk_id, flex.description&lt;br/&gt;union&lt;br/&gt;select arc.attribute4 AS area_code,&lt;br/&gt;arc.attribute4 AS description,&lt;br/&gt;arc.attribute4 || &apos; &apos; || flex.description AS area_label,&lt;br/&gt;aur.user_name AS user_name,           &lt;br/&gt;arc.attribute1 AS cust_group_code,&lt;br/&gt;arc.attribute3 AS region_code,&lt;br/&gt;prod.promo_produk_id AS promo_produk_id,&lt;br/&gt;null AS proposal_id&lt;br/&gt;from apps.ar_customers arc, app_user_region aur, PROP_REGION pg, PROMO_PRODUK prod, APPS.FCS_FLEX_VALUES_VL flex&lt;br/&gt;where arc.attribute3 = pg.region_code&lt;br/&gt;and aur.region_code = arc.attribute3&lt;br/&gt;and arc.status = &apos;A&apos;&lt;br/&gt;and prod.proposal_id = pg.proposal_id&lt;br/&gt;and flex.flex_value = arc.attribute4&lt;br/&gt;group by arc.attribute4, arc.attribute1, arc.attribute3, aur.user_name, prod.promo_produk_id, flex.description&lt;br/&gt;order by 1</userDefinedSQL>
