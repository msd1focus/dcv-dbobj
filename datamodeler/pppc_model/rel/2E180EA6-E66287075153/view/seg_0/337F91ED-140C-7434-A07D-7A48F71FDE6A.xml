<TableView class="oracle.dbtools.crest.model.design.relational.TableView" directorySegmentName="seg_0" id="337F91ED-140C-7434-A07D-7A48F71FDE6A" schemaObject="7CBB1AB6-5E3C-CEFF-688A-822A0E866097" name="FCS_SHUTTLE_CUST_GROUP">
