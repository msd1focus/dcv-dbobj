<TableView class="oracle.dbtools.crest.model.design.relational.TableView" directorySegmentName="seg_0" id="17D23012-24B5-53C5-EADA-837372019AF2" schemaObject="7CBB1AB6-5E3C-CEFF-688A-822A0E866097" name="FCS_RPT_VIEW_JOIN">
